<p id="notice"><%= notice %></p>

<h1>Listing State Drought Pres</h1>

<table>
  <thead>
    <tr>
      <th>Climate</th>
      <th>State abbreviation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_drought_pres.each do |state_drought_pre| %>
      <tr>
        <td><%= state_drought_pre.climate_id %></td>
        <td><%= state_drought_pre.state_abbreviation %></td>
        <td><%= link_to 'Show', state_drought_pre %></td>
        <td><%= link_to 'Edit', edit_state_drought_pre_path(state_drought_pre) %></td>
        <td><%= link_to 'Destroy', state_drought_pre, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State drought pre', new_state_drought_pre_path %>
