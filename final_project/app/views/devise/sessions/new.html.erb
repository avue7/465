<div class="back_home">
  <%= link_to "Back To Home Page", root_path %>
</div>

<center><div class="header_sign_up">
  <h1>Log in</h1>
</div></center>

<center>
<div id="sign_up_box">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <center>
  <%= devise_error_messages! %>
  <div class="notice">
  <center>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
  </center>
  </div>
  <div class="field">
    <%= f.email_field :email, placeholder:"ENTER YOUR EMAIL HERE", autocomplete:"off", autofocus: true %>
  </div>

  <div class="field">
    <%= f.password_field :password, placeholder:"ENTER YOUR PASSWORD HERE", autocomplete: "off" %>
  </div>
  <% if devise_mapping.rememberable? -%>
   <p><%= f.check_box :remember_me %>Remember me </p>
  <% end -%>
  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
  </center>
<% end %>
<%= render "devise/shared/links" %>
</div>
